{
  "name": "@jayfate/path",
  "version": "0.0.14-beta.1",
  "description": "The cross-platform `path` module. Uses `/` as the path separator for path parsing  and returned values on `Windows/MacOS/Linux`.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    "import": "./build/index.mjs",
    "default": "./build/index.js"
  },
  "unpkg": "build/index.min.js",
  "files": [
    "build/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "es": "tsc",
    "build": "rollup -c",
    "generate": "npm run clean && npm run es && npm run build",
    "clean": "rimraf index.*",
    "prepublishOnly": "npm run generate",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm generate && pnpm release:only",
    "release:only": "changeset publish --registry=https://registry.npmjs.com/"
  },
  "keywords": [
    "path",
    "windows"
  ],
  "repository": {
    "url": "https://github.com/JayFate/path"
  },
  "author": "JayFate",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@rollup/plugin-typescript": "^8.3.4",
    "@types/jest": "^29.2.0",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.7.23",
    "jest-environment-jsdom": "^29.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.78.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "tslib": "^2.4.0"
  }
}
